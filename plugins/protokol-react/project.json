{
  "name": "protokol-react",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "plugins/protokol-react/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:swc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/plugins/protokol-react",
        "main": "plugins/protokol-react/src/index.ts",
        "tsConfig": "plugins/protokol-react/tsconfig.lib.json",
        "assets": [
          "plugins/protokol-react/*.md",
          {
            "input": "./plugins/protokol-react/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./plugins/protokol-react/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./plugins/protokol-react",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./plugins/protokol-react",
            "glob": "executors.json",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "plugins/protokol-react/**/*.ts",
          "plugins/protokol-react/package.json",
          "plugins/protokol-react/generators.json"
        ]
      }
    }
  },
  "tags": []
}
